var canConstruct = function (ransomNote, magazine) {
  var minLength = 1;
  var maxLength = 100000;
  var magazineObject = {};
  for (const char of magazine) {
    var flag = false;
    for (const key in magazineObject) {
      if (char == key) {
        magazineObject[char] = magazineObject[char] + 1;
        flag = true;
        break;
      }
    }
    if (flag == false) magazineObject[char] = 1;
  }

  for (const char of ransomNote) {
    var hasCharInObject = false;
    for (const key in magazineObject) {
      if (char === key) {
        if (magazineObject[char] > 0) {
          magazineObject[char]--;
          hasCharInObject = true;
        } else return false;
      }
    }
    if (hasCharInObject == false) return false;
  }
  return true;
};

var ransomNote = "a",
  magazine = "aaaabbb";
console.log(canConstruct(ransomNote, magazine));
